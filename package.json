  "license": "Apache-2.0",
  "dependencies": {
    "@material/top-app-bar": "^4.0.0",
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-browsersync": "^1.1.0",
    "material-components-web": "^4.0.0",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "cordova-electron": "^1.1.1",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
